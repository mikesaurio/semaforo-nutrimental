<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="curisteando.com.semaforonutrimental">

    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splash"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="curisteando.com.barcodez.ScannerActivity"></activity>
        <activity android:name="curisteando.com.barcodez.ScannerFragmentActivity"></activity>
        <activity android:name="curisteando.com.barcodez.SimpleScannerActivity"></activity>
        <activity android:name="curisteando.com.barcodez.SimpleScannerFragmentActivity"></activity>

        <activity android:name="curisteando.com.semaforonutrimental.activities.MainActivity"></activity>
        <activity android:name="curisteando.com.semaforonutrimental.activities.CapturaDatosActivity"></activity>
        <activity android:name="curisteando.com.semaforonutrimental.activities.ResultadosNutricionActivity"></activity>
    </application>

</manifest>
